---
import Playstore from "../images/playstore.svg";
import Appstore from "../images/appstore.svg";
---

<section class="mycta2" id="cta2">
  <div class="container" data-aos="fade-up">
    <header class="section-header">
      <h1 class="cta2header">Get started</h1>
      <h5>Download the SHARP app</h5><br />
      <div class="mx-auto">
        <a
          href="https://apps.apple.com/gh/app/sharp-serpros/id1618687615"
          target="_blank"
        >
          <img src={Appstore} /></a
        ><a
          href="https://play.google.com/store/apps/details?id=com.donwudi.sharp_artisans&hl=en&gl=US"
          target="_blank"><img src={Playstore} style="max-width: 150px" /></a
        >
      </div>
    </header>
  </div>
</section>
